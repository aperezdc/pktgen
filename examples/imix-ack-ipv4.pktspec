tcpip-ack {
	ip {
		.src "8.8.4.4"
		.dst "1.1.21.68"

		# This element adds to the stream the TCP ACKs for the packets
		# generated by its inner element. In this case: an IMIX stream
		# plus its correspinding associated ACKs from the destination
		# to the source host.
		spec-file {
			.name "imix-simple"
		}
	}
}
